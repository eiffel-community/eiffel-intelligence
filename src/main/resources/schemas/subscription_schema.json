{
       "$schema":"http://json-schema.org/draft-04/schema#",
       "javaType":"com.ericsson.ei.controller.model.Subscription",
       "title": "Subscription",
       "description": "Subscription Schema",
       "type":"object",
       "properties":{
          "aggregationtype":{
             "type":"string"
          },
          "notificationMeta":{
             "type":"string"
          },
          "notificationType":{
             "type":"string"
          },
          "restPostBodyMediaType":{
                       "type":"string"
                    },
          "notificationMessageKeyValues":{
                       "items":{
                              "properties":{
                                 "formkey":{
                                    "type":"string"
                                 },
                                 "formvalue":{
                                     "type":"string"
                                            }
                              },
                              "type":"object"
                           },
                           "type":"array"
          },
          "repeat":{
             "type":"boolean"
          },
          "requirements":{
             "items":{
                "properties":{
                   "conditions":{
                      "items":{
                         "properties":{
                            "jmespath":{
                               "type":"string"
                            }
                         },
                         "type":"object"
                      },
                      "type":"array"
                   }
                },
                "type":"object"
             },
             "type":"array"
          },
          "subscriptionName":{
             "type":"string"
          },
          "userName":{
             "type":"string"
          },
          "password":{
             "type":"string"
          },
          "ldapUserName":{
             "type":"string"
          }
       },
       "required": ["notificationMeta", "notificationType", "subscriptionName", "requirements"]
 }
